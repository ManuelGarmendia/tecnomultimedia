void setup(){

size(400,400);
  
  background(255);
  strokeWeight(3);
  
  
}

void draw(){
background(255);
  for( int i = 10; i<400; i = i+10){
    noFill();  
    square( 0,0,i);
  }


}
void keyPressed(){

if ( key == ' ');{
stroke(random(0,255), random(0,255), random(0,255));
square(0,0,0);



}
  
}
